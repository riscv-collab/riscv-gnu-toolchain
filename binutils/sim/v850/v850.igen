  TRACE_ALU_INPUT1 (VR[vreg2]);
  v = VR[vreg2] & 0xffffffff;
  if (v & (1 << 31))
    {
      if (v == 0x80000000)
        i = 31;
      else if (v == 0xffffffff)
        i = 0;
      else
        for (i = 31; i; --i)
          if ((v & (1 << i)) == 0)
            break;
    }
  else
    {
      if (v == 0x7fffffff)
        i = 31;
      else if (v == 0x0)
        i = 0;
      else
        for (i = 31; i; --i)
          if (v & (1 << i))
            break;
    }
  VR[vreg3] = 31 - i;
  TRACE_ALU_RESULT1 (VR[vreg3]);
}

