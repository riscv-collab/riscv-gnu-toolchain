    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"amd-dbgapi >= 0.68.0\""; } >&5
  ($PKG_CONFIG --exists --print-errors "amd-dbgapi >= 0.68.0") 2>&5
